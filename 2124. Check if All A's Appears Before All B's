class Solution {
    public boolean checkString(String s) {
        int cnt=0;
        for(int i=1;i<s.length();i++){
            char ch1=s.charAt(i-1);
            char ch2=s.charAt(i);
            if(ch1=='b' && ch2=='a') cnt++;
        }
        return cnt==0;
    }
}
//The string should have 0 number of 'ba'.
