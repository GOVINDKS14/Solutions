class Solution {
    public String reverseWords(String s) {
        StringBuilder sb = new StringBuilder("");
        String splited[] = s.split("\\s+");
        for(int i=splited.length-1;i>=0;i--){
            sb.append(splited[i]+" ");
        }
        return sb.toString().trim();
    }
}
//Split the array by words and then do the useful.
