class Solution {
    public String finalString(String s) {
        StringBuilder sb = new StringBuilder("");
        for(int i=0;i<s.length();i++){
            char ch = s.charAt(i);
            if(ch=='i'){
                sb.reverse();
                continue;
            }
            else sb.append(ch);
        }
        return sb.toString();
    }
}
// Traverse the string and if the character is 'i', then reverse the StringBuilder and continue(skip). If the character is not 'i', append it.
