class Solution {
    public int accountBalanceAfterPurchase(int purchaseAmount) {
        // if(purchaseAmount==0) return 100;
        // String amount=Integer.toString(purchaseAmount);
        // int lastDigit = Character.getNumericValue(amount.charAt(amount.length()-1));
        // if(lastDigit>=1 && lastDigit<=4) return 100-(purchaseAmount-lastDigit);
        // return 100-(purchaseAmount+(10-lastDigit));
        
        //Using hint
        return 100-(int)Math.floor((purchaseAmount+5)/10)*10;
        
    }
}
